        <div class="row">
            <div class="col-sm-12">
                <div class="card">
                    <div class="card-header">
                        <h5>Basic Component</h5>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <form action="/Admin/AddLiterature" method="post">
                                <div class="col-sm-12">
                                    <h2>Literature</h2>
                                    <div>
                                      
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Title">Title</label>
                                                <input name="Title" type="text" class="form-control" id="Title" placeholder="Enter title">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Author">Author</label>
                                                <input name="Author" type="text" class="form-control" id="Author" placeholder="Enter author">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Publisher">Publisher</label>
                                                <input name="Publisher" type="text" class="form-control" id="Publisher" placeholder="Enter publisher">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Publication_Date">Publication
                                                    Date</label>
                                                <input name="Publication_Date" type="text" class="form-control" id="Publication_Date" placeholder="Enter publication date">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="ISBN">ISBN</label>
                                                <input name="ISBN" type="text" class="form-control" id="ISBN" placeholder="Enter ISBN">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Additional_Info">Additional Info</label>
                                                <input name="Additional_Info" type="text" class="form-control" id="Additional_Info" placeholder="Enter additional info">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Object_ID">Object ID</label>
                                                <select name="Object_ID" class="form-control" id="Object_ID">
                                                    <option value="">Select an object ID</option>
                                                    @foreach (var objectId in Model.galleryObjectsAdapters)
                                                    {
                                                        <option value="@objectId.Object_ID">@objectId.title</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <button type="submit" class="btn btn-primary">Add Literature</button>
                                </div>
                            </form>
                            <form  action="/Admin/AddImages" method="post">
                                <div class="col-sm-12">
                                    <h2>Image</h2>
                                    <div>                                 
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="ImageLoc">Image Location</label>
                                                <input name="ImageLoc" type="text" class="form-control" id="ImageLoc" placeholder="Enter Image Location">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Photographer">Photographer</label>
                                                <input name="Photographer" type="text" class="form-control" id="Photographer" placeholder="Enter Photographer">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="DateTaken">Date Taken</label>
                                                <input name="Date_Taken" type="text" class="form-control" id="DateTaken" placeholder="Enter Date Taken">
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="AdditionalInfo">Additional Info</label>
                                                <input name="Additional_Info" type="text" class="form-control" id="AdditionalInfo" placeholder="Enter Additional Info">
                                            </div>
                                        </div>
                                         <div class="col-sm-12">
                                            <div class="form-group">
                                                <label class="floating-label" for="Object_ID">Object ID</label>
                                                <select name="Object_ID" class="form-control" id="Object_ID">
                                                    <option value="">Select an object ID</option>
                                                    @foreach (var objectId in Model.galleryObjectsAdapters)
                                                    {
                                                            <option value="@objectId.Object_ID">@objectId.title</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <button type="submit" class="btn btn-primary">Add Image</button>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="card">
                        <div class="card-header">
                            <h5>Basic Component</h5>
                        </div>
                        <div class="card-body">
                            <form action="/Admin/AddObject" method="post">
